@model IEnumerable<Cookies.Models.Price>

@{
    Layout = null;
}


<table class="table table-hover text-nowrap" id="table2" style="width:100%">
    <thead>
        <tr>
            <th class="hide">
                @Html.DisplayNameFor(model => model.pr_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pr_price_type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pr_price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pr_start_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pr_cre_by_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pr_cre_date)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td class="hide">
                @Html.DisplayFor(modelItem => item.pr_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pr_price_type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pr_price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pr_start_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pr_cre_by_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pr_cre_date)
            </td>
         
        </tr>
}
    </tbody>
</table>
